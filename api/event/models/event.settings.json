{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "dateStart": {
      "type": "datetime",
      "required": true
    },
    "dateEnd": {
      "type": "datetime"
    },
    "url": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
